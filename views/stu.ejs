<body class="">
    <!-- Navebar -->
    <ul class="nav nav-pills justify-content-center" style="background-color : white" >
  <li class="nav-item">
    <a class="nav-link active text-black-100">Students</a>
  </li>
  <li class="nav-item">
    <a class="nav-link text-black-100" href="/">Home</a>
  </li>
  </ul>
    <!-- navigation ends here-->
<div class='container mt-4'>
    <div class="col">
        <%let i=0; students.forEach((student, i)=>{%>
        <div class="row">
            <div class="list" style="width: 18rem;">
            <div class="list-body">
                <h5 class="card-title"><strong>Student Name:</strong> <%= student.Name %></h5>
               <!-- <h5 class="card-text"><strong>dob:</strong> <%= student.dob %></h5> -->
               <!-- <h5 class="card-text"><strong>program</strong> <%= student.program %></h5>
                <h5 class="card-text"><strong>level:</strong> <%= student.level %></h5> -->
                <a href="/stuff/<%= i %>" class="btn btn-sm btn-outline-dark">Go to Details --></a>
                </div>
            </div>
         </div>
        <% }) %>
    </div>
</div>
</body>
</html>
    